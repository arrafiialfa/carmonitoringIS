{
    "version": 2,
    "builds": [
        {
            "src": "public/**",
            "use": "@vercel/static-build"
        },
        {
            "src": "/.*",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/(.*\\.php)",
            "dest": "/$1"
        },
        {
            "src": "/.*",
            "dest": "/index.php"
        }
    ]
}
